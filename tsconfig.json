{
    "compilerOptions": {
        "declaration": false,
        "experimentalDecorators": true,
        "inlineSourceMap": false,
        "inlineSources": false,
        // "mapRoot": "http://localhost/",//PATH or URL
        "maxNodeModuleJsDepth": 1,
        "module": "commonjs",
        "noEmit": false,
        "noEmitOnError": false,
        "noFallthroughCasesInSwitch": false,
        "noImplicitAny": true,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noImplicitUseStrict": false,
        "noUnusedLocals": false,
        "noUnusedParameters": false,
        "outDir": "./bin",
        // "outFile": "",
        "preserveConstEnums": false,
        "pretty": true,
        "removeComments": false,
        "rootDir": "./src",
        "sourceMap": false,
        // "sourceRoot": "http://localhost/",//PATH or URL
        "strictNullChecks": true,
        "target": "es6"
    },
    "exclude": [
        "bin",
        "lib",
        "node_modules/gots/src",
        "node_modules/tslabs/src"
    ]
}